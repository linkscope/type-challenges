{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowJs": false,
    "jsx": "react"
  },
  "exclude": ["node_modules"]
}